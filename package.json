{
  "name": "@cspotcode/zx",
  "version": "5.0.0",
  "description": "A tool for writing better scripts.",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./import.mjs",
      "require": "./index.js"
    },
    "./globals": {
      "import": "./globals.mjs",
      "require": "./globals-require.js"
    }
  },
  "bin": {
    "zx": "zx.mjs"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "build": "swc -o index.js index.mjs",
    "test": "node zx.mjs test.mjs"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0",
    "@types/node-fetch": "^3.0.2",
    "chalk": "^5.0.0",
    "fs-extra": "^10.0.0",
    "globby": "^13.1.1",
    "minimist": "^1.2.5",
    "node-fetch": "^3.2.0",
    "ps-tree": "^1.2.0",
    "which": "^2.0.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.49",
    "@swc/core": "^1.2.80"
  },
  "files": [
    "*.mjs",
    "!index.mjs",
    "*.js",
    "*.d.ts"
  ],
  "repository": "cspotcode/zx",
  "author": "Anton Medvedev <anton@medv.io>",
  "license": "Apache-2.0"
}
